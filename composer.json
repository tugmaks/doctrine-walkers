{
    "name": "tugmaks/doctrine-walkers",
    "description": "Set of doctrine walkers",
    "license": [
        "MIT"
    ],
    "type": "library",
    "keywords": [
        "doctrine-walker",
        "output-walker",
        "doctrine",
        "skip-locked",
        "nulls-last",
        "nulls-first",
        "tablesample"
    ],
    "authors": [
        {
            "name": "Maxim Tyugaev",
            "email": "tugmaks@yandex.ru"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/orm": "^2.20"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.45",
        "ergebnis/php-cs-fixer-config": "^6.42",
        "friendsofphp/php-cs-fixer": "^3.68",
        "icanhazstring/composer-unused": "^0.8.10",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/phpunit": "^10.4"
    },
    "autoload": {
        "psr-4": {
            "Tugmaks\\DoctrineWalkersTest\\": "tests/",
            "Tugmaks\\DoctrineWalkers\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
