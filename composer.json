{
    "name": "tugmaks/doctrine-walkers",
    "license": [
        "MIT"
    ],
    "type": "library",
    "keywords": [
        "doctrine-walker",
        "output-walker",
        "doctrine"
    ],
    "authors": [
        {
            "name": "Maxim Tyugaev",
            "email": "tugmaks@yandex.ru"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/orm": "^2.15"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.39",
        "ergebnis/php-cs-fixer-config": "^6.10",
        "friendsofphp/php-cs-fixer": "^3.37",
        "icanhazstring/composer-unused": "^0.8.10",
        "phpunit/phpunit": "^10.4",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Tugmaks\\DoctrineWalkersTest\\": "tests/",
            "Tugmaks\\DoctrineWalkers\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
